{
  "$schema": "../chain.schema.json",
  "chain_name": "sultan",
  "status": "live",
  "network_type": "mainnet",
  "website": "https://sultan-l1.network",
  "pretty_name": "Sultan L1",
  "chain_id": "sultan-1",
  "bech32_prefix": "cosmos",
  "daemon_name": "sultand",
  "node_home": "$HOME/.sultan",
  "key_algos": [
    "secp256k1"
  ],
  "slip44": 118,
  "fees": {
    "fee_tokens": [
      {
        "denom": "stake",
        "fixed_min_gas_price": 0,
        "low_gas_price": 0,
        "average_gas_price": 0,
        "high_gas_price": 0
      }
    ]
  },
  "staking": {
    "staking_tokens": [
      {
        "denom": "stake"
      }
    ]
  },
  "codebase": {
    "git_repo": "https://github.com/wollnbergen/0xv7",
    "recommended_version": "v1.0.0",
    "compatible_versions": [
      "v1.0.0"
    ],
    "cosmos_sdk_version": "v0.50.6",
    "ibc_go_version": "v8.0.0",
    "consensus": {
      "type": "cometbft",
      "version": "v0.38.11"
    },
    "binaries": {
      "linux/amd64": "https://github.com/wollnbergen/0xv7/releases/download/v1.0.0/sultand-linux-amd64"
    }
  },
  "logo_URIs": {
    "png": "https://raw.githubusercontent.com/cosmos/chain-registry/master/sultan/images/sultan.png",
    "svg": "https://raw.githubusercontent.com/cosmos/chain-registry/master/sultan/images/sultan.svg"
  },
  "description": "Sultan L1 - Rust-powered blockchain with Cosmos SDK integration. Features zero-fee transactions, IBC v8 support, and quantum-resistant cryptography.",
  "peers": {
    "seeds": [],
    "persistent_peers": []
  },
  "apis": {
    "rpc": [
      {
        "address": "http://localhost:26657",
        "provider": "sultan"
      }
    ],
    "rest": [
      {
        "address": "http://localhost:1317",
        "provider": "sultan"
      }
    ],
    "grpc": [
      {
        "address": "localhost:9090",
        "provider": "sultan"
      }
    ]
  },
  "explorers": [
    {
      "kind": "mintscan",
      "url": "https://www.mintscan.io/sultan",
      "tx_page": "https://www.mintscan.io/sultan/txs/${txHash}",
      "account_page": "https://www.mintscan.io/sultan/account/${accountAddress}"
    }
  ],
  "keywords": [
    "rust",
    "cosmos",
    "ibc",
    "zero-fees",
    "quantum-resistant",
    "l1"
  ]
}
